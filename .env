#Port of the application to listen at
APP_PORT=2000

#Application URL, example: https://client.example.com
APP_URL="http://localhost:2000"

#Application environment type, available options: production, development and test.
APP_ENV=production

#Log debugs directly to console?
APP_DEBUG=true

#Application native language
APP_LOCALE=en

#Enable maintenance mode?
APP_MAINTENANCE=true

#Enable telemetry?
APP_TELEMETRY=true

#Application web secret, leave it as random to generate a random string.
APP_SECRET=random

DB_CONNECTION=MySQL
MYSQL_DB_HOST=127.0.0.1
MYSQL_DB_PORT=3306
MYSQL_DB_DATABASE=holaclient
MYSQL_DB_USERNAME=holaclient
MYSQL_DB_PASSWORD=holaclient
